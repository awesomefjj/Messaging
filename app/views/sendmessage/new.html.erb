<h1>Sing up</h1>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-V06iGeOYiHqaJG18vU/udsyVfgcm8Pgax9HmoZh65R0FrT9X2GZZ8w2ZQcZkzZGV" crossorigin="anonymous">
<%= form_with model: @send,  url:sign_up_path do |form| %>

<div class="mb-3">
    <%= form.label :title %>
 <%= form.text_field :title, class: "form-control"  %>
 </div>
<div class="mb-3">
    <%= form.label :kind %>
    <%= form.text_field :kind, class: "form-control"  %>
    </div>
<div class="mb-3">
    <%= form.label :status %>
        <%= form.text_field :status, class: "form-control"  %>
        </div>
        <div class="mb-3">
            <%= form.label :event_id %>
            <%= form.text_field :event_id, class: "form-control"  %>
            </div>        
<div class="mb=3">
    <%= form.label :content %>
 <%= form.text_field :content, class: "form-control" %>
</div>
<div class="mb3 ">
    <%= form.label :created_by %>
<%= form.text_field :created_by, class: "form-control" %>
<div class="mb-3">
    <%= form.submit "send", class: "btn btn-primary"%>
</div>
<% end %>
</div>